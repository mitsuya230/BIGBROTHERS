pip install selenium
from selenium import webdriver
import unittest

class TestVideoUpload(unittest.TestCase):
    def setUp(self):
        self.driver = webdriver.Chrome(executable_path="path_to_chromedriver")
    
    def test_video_upload(self):
        driver = self.driver
        driver.get("https://rutube.ru/upload")

        # Загружаем видео
        upload_button = driver.find_element_by_id("upload-button")
        upload_button.send_keys("path_to_video.mp4")
        
        # Вводим данные видео
        title_input = driver.find_element_by_id("video-title")
        title_input.send_keys("Test Video")
        
        description_input = driver.find_element_by_id("video-description")
        description_input.send_keys("This is a test video.")

        # Нажимаем кнопку "Загрузить"
        submit_button = driver.find_element_by_id("submit-button")
        submit_button.click()

        # Проверяем, что видео загрузилось
        success_message = driver.find_element_by_xpath("//div[@class='upload-success']")
        self.assertTrue(success_message.is_displayed())

    def tearDown(self):
        self.driver.quit()

if __name__ == "__main__":
    unittest.main()
